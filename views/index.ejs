<% include ./partials/header %>
<p>
  <% blogs.forEach((blog) => { %>
    <div>
      <h2><%= blog.title %></h2>
      <img src='<%= blog.image %>'/>
      <span><%= blog.created %></span>
      <p><%= blog.body.substring(0, 100) %>...</p>
      <a href='/blogs/<%= blog._id %>'>Read more</a>
    </div>
  <% }) %>
</p>
<% include ./partials/footer %>
